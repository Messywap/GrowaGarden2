-- üîÅ Server Hopper
local HttpService = game:GetService("HttpService")
local TeleportService = game:GetService("TeleportService")
local Players = game:GetService("Players")
local StarterGui = game:GetService("StarterGui")

local function safeTeleport(placeId, jobId)
	for attempt = 1, 3 do
		local success, result = pcall(function()
			TeleportService:TeleportToPlaceInstance(placeId, jobId, Players.LocalPlayer)
		end)
		if success then return true else task.wait(1) end
	end
	return false
end

local function hopToPublicServer()
	local placeId = game.PlaceId
	local currentJobId = game.JobId
	local cursor = nil

	for _ = 1, 10 do
		local url = "https://games.roblox.com/v1/games/" .. placeId .. "/servers/Public?sortOrder=Asc&limit=100"
		if cursor then url = url .. "&cursor=" .. cursor end

		local success, result = pcall(function()
			return HttpService:JSONDecode(game:HttpGet(url))
		end)

		if success and result and result.data then
			for _, server in ipairs(result.data) do
				if server.playing < server.maxPlayers and server.id ~= currentJobId then
					if safeTeleport(placeId, server.id) then return end
				end
			end
			cursor = result.nextPageCursor
		else
			break
		end
	end

	StarterGui:SetCore("SendNotification", {
		Title = "Server Hopper",
		Text = "‚ùå Couldn't find any public servers.",
		Duration = 4
	})
end

-- üëë Other Tab
local TabOther = Window:CreateTab("üëë Other")

TabOther:CreateSection("ü•∞ Join Old Servers")
TabOther:CreateDropdown({
   Name = "Game Version",
   Options = {"Version 1235", "Version 1229", "Version 1214"},
   CurrentOption = {"Version 1235"},
   MultiSelection = false,
   Flag = "DropdownVersion",
   Callback = function(Option)
      print("Selected Version:", Option)
   end,
})

TabOther:CreateButton({
   Name = "Join Server",
   Callback = function()
      Rayfield:Notify({
         Title = "Server Hopper",
         Content = "üîÑ Finding public server...",
         Duration = 5,
         Image = 4483362458,
      })
      task.wait(1)
      hopToPublicServer()
   end,
})

